        <Label x:Name="jpy_Copy" Grid.Column="2" Content="Label" HorizontalAlignment="Left" Margin="147,517,0,0" VerticalAlignment="Top" Width="112" FontFamily="Cambria" FontStyle="Italic"/>
        <Label x:Name="eur_Copy" Grid.Column="2" Content="Label" HorizontalAlignment="Left" Margin="147,558,0,0" VerticalAlignment="Top" Width="128" FontFamily="Cambria" FontStyle="Italic"/>
        <Label Grid.Column="2" Content="$" HorizontalAlignment="Left" Margin="138,480,0,0" VerticalAlignment="Top" FontFamily="Cambria" FontStyle="Italic"/>
        <Label Grid.Column="2" Content="€" HorizontalAlignment="Left" Margin="138,558,0,0" VerticalAlignment="Top" FontFamily="Cambria" FontStyle="Italic"/>
        <Label Grid.Column="2" Content="¥" HorizontalAlignment="Left" Margin="138,517,0,0" VerticalAlignment="Top" FontFamily="Cambria" FontStyle="Italic"/>
