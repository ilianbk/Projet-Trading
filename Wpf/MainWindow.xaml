                <ImageBrush/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="date1" HorizontalAlignment="Left" Margin="118,340,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="220" Foreground="Black" Grid.ColumnSpan="2" Height="22" BorderThickness="0,0,0,0" FontFamily="Cambria" FontStyle="Italic">
