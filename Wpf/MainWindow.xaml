        </TextBox>
        <TextBox x:Name="date2" HorizontalAlignment="Left" Margin="117,502,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="221" Foreground="Black" Grid.ColumnSpan="2" Height="21" BorderThickness="0,0,0,0" FontFamily="Cambria" FontStyle="Italic">
            <TextBox.Background>
                <ImageBrush/>
            </TextBox.Background>
        </TextBox>
        <TextBox x:Name="title1" HorizontalAlignment="Left" Margin="117,285,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="221" Foreground="Black" Grid.ColumnSpan="2" Height="43" BorderThickness="0,0,0,0" FontFamily="Cambria" FontStyle="Italic" FontWeight="Bold" FontSize="14">
